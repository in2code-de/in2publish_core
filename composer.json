{
	"name": "in2code/in2publish_core",
	"description": "Content publishing extension to connect stage and production server",
	"keywords": [
		"TYPO3 CMS",
		"publishing",
		"publisher",
		"stage",
		"content",
		"multiserver",
		"workflow",
		"workspace",
		"security",
		"publish",
		"synchronization",
		"stage",
		"staging",
		"contentstaging",
		"headless",
		"bodyless",
		"deployment"
	],
	"authors": [
		{
			"name": "Oliver Eglseder",
			"email": "oliver.eglseder@in2code.de"
		},
		{
			"name": "Alex Kellner",
			"email": "alexander.kellner@in2code.de"
		},
		{
			"name": "Stefan Busemann",
			"email": "stefan.busemann@in2code.de"
		}
	],
	"type": "typo3-cms-extension",
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.0",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-zip": "*",
		"typo3/cms-core": "^12.4",
		"symfony/yaml": "^v5.4 || ^v6.3"
	},
	"autoload": {
		"psr-4": {
			"In2code\\In2publishCore\\": "Classes"
		},
		"files": [
			"constants.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"In2code\\In2publishCore\\Tests\\": "Tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-develop": "12.7.x-dev",
			"dev-master": "12.7.x-dev"
		},
		"typo3/cms": {
			"extension-key": "in2publish_core"
		}
	}
}
